    "babel-loader": "^8.2.3",
    "chromatic": "^6.0.4",
    "css-loader": "^6.4.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.26.1",
    "html-webpack-plugin": "^5.4.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
