  "dependencies": {
    "@emotion/css": "^11.5.0",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.4.4"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint ./src --fix"
    ],
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "./src/**/*.{js,jsx,ts,tsx}",
      "!./src/**/*.stories.{js,jsx,ts,tsx}"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/settings/JestSetup.js"
    ]
